/* USER CODE BEGIN 4 */
static void led (void)
{
	//Configure the MODE register
		//01: General Purpose output
		//MODER Bit 29 and Bit 28
		//GPIOB->MODER &= ~(1 << 29);
		//GPIOB->MODER |= (1 << 28);
		//RESET both 29 and 28 bits to '0'
		GPIOB->MODER &= ~(0x3 << 28);
		//SET the values
		GPIOB->MODER |= (0x1 << 28);
		//Configure Output Type Register
		//0: Push-Pull
		//OTYPER Bit 14
		GPIOB->OTYPER &= ~(1 << 14);

		//Configure the Speed Reg
		//11: Very High Speed
		//OSPEEDER Bit 29 and Bit 28
		//GPIOB->OSPEEDER &= ~(1 << 29);
		//GPIOB->OSPEEDER &= ~(1 << 28);
		//RESET both 29 and 28 bits to '0'
		GPIOB->OSPEEDR &= ~(0x3 << 28);
		//SET the values
		GPIOB->OSPEEDR |= (0x3 << 28);

		//Configure Pull-up/Pull-down reg
		//00: No pull-up/pull-down
		//PUPDR Bit 29 and Bit 28
		//RESET both 29 and 28 bits to '0'
		GPIOB->PUPDR &= ~(0x3 << 28);
		//SET the values
		GPIOB->PUPDR |= (0x0 << 28);
}
static void SensorRead (void * parameter)
{
	char tempReading[100];
	float tempValue = 0;

	char humidityReading[100];
	double humidityValue = 0;

	char pressureReading[100];
	float pressureValue = 0;

	char acceloReading[100];
	//float acceloValue = 0;
	char gyroReading[100];

	char magnetoReading[100];




	while(1){

		printf("\r\n Entered the loop \r\n");
		tempValue = BSP_TSENSOR_ReadTemp();
		led();
		int tempValueInt = tempValue;
		float tempValueFrac = 0;

		tempValueFrac = tempValue - tempValueInt;

		int tempValueFrac2digits = trunc (tempValueFrac * 100);
		printf("\r\n Printing Temperature value....\r\n");

		snprintf(tempReading, 100 , "\r\n TEMPERATURE = %1d.%2d  \n\r", tempValueInt, tempValueFrac2digits);
		if(tempValueInt > 30)
		{
			led();
			printf(" \r\n Temperature crossed threshold value....\r\n");
			HAL_GPIO_TogglePin(GPIOA, GPIO_PIN_4);
			HAL_Delay(5000);

		}


		HAL_UART_Transmit(&huart1, (uint8_t *) tempReading, strlen(tempReading) , 1000);
		printf("\r\n Done with Temperature Value \r\n");

		HAL_Delay(5000);

		humidityValue = BSP_HSENSOR_ReadHumidity();
		int humidityValueInt = humidityValue;
		float humidityValueFrac = 0;
		humidityValueFrac = humidityValue - humidityValueInt;
		int humidityValueFrac2digits = trunc(humidityValueFrac * 100);
		snprintf(humidityReading, 100, "\r\n HUMIDITY = %1d. %2d \n\r", humidityValueInt, humidityValueFrac2digits);
		HAL_UART_Transmit(&huart1, (uint8_t *)humidityReading, strlen(humidityReading), 1000);

		if(humidityValueInt > 90)
				{
					led();
					printf(" \r\n Humidity crossed threshold value....\r\n");
					HAL_GPIO_TogglePin(GPIOA, GPIO_PIN_4);
					HAL_Delay(500);

				}
		HAL_Delay(5000);

		pressureValue = BSP_PSENSOR_ReadPressure();
		int pressureValueInt = pressureValue;
		float pressureValueFrac = 0;

		pressureValueFrac = pressureValue - pressureValueInt;

		int pressureValueFrac2digits = trunc (pressureValueFrac * 100);
		printf("\r\n Printing Pressure value....\r\n");

		snprintf(pressureReading, 100 , "\r\n PRESSURE = %1d.%2d  \n\r", pressureValueInt, pressureValueFrac2digits);

		HAL_UART_Transmit(&huart1, (uint8_t *) pressureReading, strlen(pressureReading) , 1000);
		printf("\r\n Done with Pressure Value \r\n");

		if(pressureValueInt > 1000)
						{
							led();
							printf(" \r\n Pressure crossed threshold value....\r\n");
							HAL_GPIO_TogglePin(GPIOB, GPIO_PIN_14);
							HAL_Delay(500);

						}
		HAL_Delay(5000);

		int16_t accelo[3];
		//ACCELERO_DrvTypeDef *a;
		BSP_ACCELERO_AccGetXYZ(accelo);
		//a->GetXYZ(&accelo);
		snprintf(acceloReading, 100, "\r\n Accelorometer = % 5d, % 5d, %5d \r\n",(int16_t) accelo[0],accelo[1], accelo[2] );
		HAL_UART_Transmit(&huart1, (uint8_t *) acceloReading, strlen(acceloReading) , 1000);

		HAL_Delay(5000);

		int gyro[3];
				//ACCELERO_DrvTypeDef *a;
		BSP_GYRO_GetXYZ((float *)gyro);
				//a->GetXYZ(&accelo);
		snprintf(gyroReading, 100, "\r\n Gyroscope = % 5d, % 5d, %5d \r\n", gyro[0]/10000000,gyro[1]/10000000, gyro[2]/10000000 );
		HAL_UART_Transmit(&huart1, (uint8_t *) gyroReading, strlen(gyroReading) , 1000);

		HAL_Delay(5000);

		int16_t magneto[3];
				//ACCELERO_DrvTypeDef *a;
		BSP_MAGNETO_GetXYZ(magneto);
				//a->GetXYZ(&accelo);
				snprintf(magnetoReading, 100, "\r\n Magnetometer = % 5d, % 5d, %5d \r\n",(int16_t) magneto[0],magneto[1], magneto[2] );
				HAL_UART_Transmit(&huart1, (uint8_t *) magnetoReading, strlen(magnetoReading) , 1000);

				HAL_Delay(5000);




	}


}
int _write(int fd, char * ptr, int len)
{
  HAL_UART_Transmit(&huart1, (uint8_t *) ptr, len, HAL_MAX_DELAY);
  return len;
}
